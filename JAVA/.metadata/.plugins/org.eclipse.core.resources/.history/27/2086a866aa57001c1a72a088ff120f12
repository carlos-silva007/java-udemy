package controle;

public class BreakRotulado {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		externo: for (int i = 0; i < 3; i++) {
			for (int j = 0; j < 5; j++) {
				if (i == 1) {
					break externo;
				}
				System.out.printf("[%d %d] ", i, j);
			}
			System.out.println();
		}
		System.out.println("Fim");
	}

}
